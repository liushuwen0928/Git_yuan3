<%@ page contentType="text/html;charset=UTF-8" language="java" %>
<%@ taglib prefix="c" uri="http://java.sun.com/jsp/jstl/core"%>
<html>
<head>
    <title>Title</title>
</head>
<body>
<table border="1" cellpadding="1">
    <tr>
        <td>id</td>
        <td>name</td>
    </tr>
    <c:forEach items="${requestScope.list}" var="p">
        <tr>
            <td>${p.id}</td>
            <td>${p.name}</td>
        </tr>
    </c:forEach>
</table>
</body>
</html>
